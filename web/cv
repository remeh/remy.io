    <script>
      function showInternships() {
        document.getElementById('show-internships').style.display = 'none';
        var internships = [].slice.call(document.getElementsByClassName('internship'));
        if (internships) {
          internships.map(function(internship) {
            internship.style.display = '';
          });
        }
        mixpanel.track('cv show internships');
      };
      function contact() {
        var str = 'abcdefghijklmnopqrstuvwxyz@.:';
        //         01234567890123456789012345678
        var a = str[12] + str[0] + str[8] + str[11] + str[19] + str[14] + str[28];
        var z = str[12] + str[4] + str[26] + str[17] + str[4] + str[12] + str[24] + str[27] + str[8] + str[14];
        document.getElementById('show-contact').style.display = 'none';
        document.getElementById('me').style.display = '';
        document.getElementById('me').innerHTML = '<a href='+a+z+'>'+z+'</a>';
      };
    </script>
